{
  "name": "fmc-website",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite dev --force",
    "test": "tsc && prettier --check . && vitest run",
    "build": "vite build",
    "preview": "vite build && vite preview",
    "format": "prettier --write .",
    "ship": "vite build && wrangler deploy",
    "types": "wrangler types --include-runtime false --strict-vars false",
    "tail": "wrangler tail"
  },
  "dependencies": {
    "@std/front-matter": "jsr:^1.0.9",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "p-limit": "^7.1.1",
    "rwsdk": "1.0.0-alpha.4",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.12.4",
    "@cloudflare/workers-types": "^4.20250913.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.13",
    "@types/markdown-it": "^14.1.2",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.5.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "daisyui": "^5.1.12",
    "dotenv": "^17.2.2",
    "fast-glob": "^3.3.3",
    "mime-types": "^3.0.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "wrangler": "4.35.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none"
  }
}
